#include "E:\Projects\Midori\MidoriUtil\MidoriUtil.mdr"

var makeAdder = fn(var a : Int, var b : Int) : (Int) -> Int 
{
  return fn(var x : Int) : Int { return a + b + x; };
};

var adder = makeAdder(3, 4);

fixed main = fn() : Int
{
	IO::PrintLine(adder(5) as Text);  // Should print 12

	return 0;
};