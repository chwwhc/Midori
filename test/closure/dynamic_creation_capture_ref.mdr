#include "E:\Projects\Midori\MidoriUtil\MidoriUtil.mdr"

fixed foo = fn() : Unit
{
	var arr : Array[() -> Int] = [];
	for (var i = 0; i < 3; i = i + 1)
	{
		arr = arr ++ [fn() : Int
		{
			return i;
		}];
	}

	IO::PrintLine(arr[0]() as Text);  // Should print 3
	IO::PrintLine(arr[1]() as Text);  // Should print 3
	IO::PrintLine(arr[2]() as Text);  // Should print 3
	
	return ();
};

fixed main = fn() : Unit
{
	foo();
	return ();
};