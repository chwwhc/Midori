var x = closure(var x)
{
    return closure(){ return x; };
};

var A = closure(var k, var x1, var x2, var x3, var x4, var x5)
{
    var B = closure()
    {
        k = k - 1;
        return A(k, B, x1, x2, x3, x4); 
    };
    return k > 0 ? B() : x4() + x5();
};

fixed main = closure()
{
    PrintLine(A(1, x(1), x(-1), x(-1), x(1), x(0)));   

    return 0;
};