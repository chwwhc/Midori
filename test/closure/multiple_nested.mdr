var makeAdder = closure(var a : Integer) : (Integer) -> (Integer) -> Integer 
{
  return closure(var b : Integer) : (Integer) -> Integer
  {
    return closure(var c : Integer) : Integer
    {
        return a + b + c;
    };
  };
};

var adder : (Integer) -> Integer = makeAdder(3)(4);

fixed main = closure() : Unit
{
    PrintLine(adder(5));  // Should print 12

    return #;
};