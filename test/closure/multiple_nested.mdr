var makeAdder = fn(var a : Int) : (Int) -> (Int) -> Int 
{
  return fn(var b : Int) : (Int) -> Int
  {
    return fn(var c : Int) : Int
    {
        return a + b + c;
    };
  };
};

var adder : (Int) -> Int = makeAdder(3)(4);

fixed main = fn() : Unit
{
    PrintLine(adder(5));  // Should print 12

    return #;
};
