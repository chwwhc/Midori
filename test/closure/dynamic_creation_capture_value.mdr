#include "E:\Projects\Midori\MidoriUtil\MidoriUtil.mdr"

fixed foo = fn() : Unit
{
	var arr : Array<() -> Int> = [];
	for (var i : Int = 0; i < 3; i = i + 1)
	{
		(fn(fixed i : Int) : Unit
		{
			arr = arr ++ [fn() : Int
			{
				return i;
			}];
			return ();
		})(i);
	}

	PrintLine(arr[0]() as Text);  // Should print 0
	PrintLine(arr[1]() as Text);  // Should print 1
	PrintLine(arr[2]() as Text);  // Should print 2
	
	return ();
};

fixed main = fn() : Unit
{
	foo();

	return ();
};