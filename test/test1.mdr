var x = function(x)
{
    return function(){ return x; };
};

var A = function(k, x1, x2, x3, x4, x5)
{
    var B = function()
    {
        k = k - 1;
        return A(k, B, x1, x2, x3, x4); //decrement k before use
    };
    return k > 0 ? B() : x4() + x5();
};

PrintLine(A(10, x(1), x(-1), x(-1), x(1), x(0))); // result: -67